{
    "name": "durimjusaj/alice-bundle",
    "description": "Symfony bundle to manage fixtures with Alice and Faker.",
    "keywords": ["Symfony", "Fixture", "ORM", "Alice", "Faker"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Baldur Rensch",
            "email": "brensch@gmail.com"
        },
        {
            "name": "ThÃ©o FIDRY",
            "email": "theo.fidry@gmail.com",
            "homepage": "https://github.com/theofidry"
        }
    ],

    "autoload": {
        "psr-4": {
            "Hautelook\\AliceBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hautelook\\AliceBundle\\": [
                "fixtures",
                "tests"
            ]
        }
    },

    "require": {
        "php": ">=8.2",
        "doctrine/doctrine-bundle": "^2.11",
        "doctrine/data-fixtures": "^1.7",
        "doctrine/orm": "^3.1",
        "doctrine/persistence": "^3.3",
        "psr/log": "^3.0",
        "symfony/finder": "^7.0",
        "symfony/framework-bundle": "^7.0",
        "theofidry/alice-data-fixtures": "^1.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpspec/prophecy": "^1.7",
        "symfony/phpunit-bridge": "^7.0",
        "phpspec/prophecy-phpunit": "^2.0",
        "monolog/monolog": "^3.5"
    },

    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "bin-dir": "bin",
        "sort-packages": true
    }
}
